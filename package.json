{
  "name": "mesh-fetcher",
  "version": "1.0.2",
  "description": "A Node.js package for fetching data from multiple APIs with enhanced features.",
  "main": "index.cjs",
  "module": "index.mjs",
  "types": "src/types/index.d.ts",
  "scripts": {
    "test": "jest"
  },
  "jest": {
    "transform": {
      "^.+\\.mjs$": "babel-jest",
      "^.+\\.js$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "mjs",
      "json",
      "node"
    ],
    "testMatch": [
      "**/tests/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[tj]s?(x)"
    ]
  },
  "keywords": [
    "API",
    "fetch",
    "debounce",
    "throttle"
  ],
  "author": "Vedas Dixit",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.8.4"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "babel-jest": "^29.7.0",
    "eslint": "^9.23.0",
    "jest": "^29.7.0",
    "typescript": "^5.3.3"
  }
}
